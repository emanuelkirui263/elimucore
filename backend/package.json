{
  "name": "elimucore-backend",
  "version": "1.0.0",
  "description": "ELIMUCORE - Kenyan High School Management Information System",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --coverage",
    "db:init": "node scripts/init-database.js",
    "db:init:force": "node scripts/init-database.js --force",
    "db:seed": "node scripts/seed-database.js",
    "db:migrate": "bash ../scripts/run-migrations.sh development",
    "db:migrate:prod": "bash ../scripts/run-migrations.sh production",
    "migrate": "sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all"
  },
  "keywords": [
    "education",
    "school",
    "management",
    "kenya"
  ],
  "author": "KIRUINEXUS",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "pdfkit": "^0.14.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2"
  }
}
